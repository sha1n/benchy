  "version": "0.2.0",
  "configurations": [
    {
      "name": "Benchy (basic)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/benchy.go",
      "args": [
        "-c",
        "${workspaceFolder}/test/data/spec_test_load.yaml"
      ]
    },
    {
      "name": "Benchy (csv/raw)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/benchy.go",
      "args": [
        "-c",
        "${workspaceFolder}/test/data/spec_test_load.yaml",
        "--format",
        "csv/raw",
        "--label=vscode"
      ]
    },
    {
      "name": "Benchy (md)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/benchy.go",
      "args": [
        "-c",
        "${workspaceFolder}/test/data/spec_test_load.yaml",
        "--pipe-stdout=false",
        "--silent",
        "--format",
        "md",
        "--label=vscode"
      ]
    }
  ]
}